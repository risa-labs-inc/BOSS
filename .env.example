# Together AI Environment Configuration
# Copy this file to .env, .env.development, .env.testing, or .env.production as needed

# Environment (development, testing, production)
ENVIRONMENT=development

# LLM API Keys - Replace with your own keys
TOGETHER_API_KEY=your_together_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_api_key_here

# Environment-specific API keys (optional)
# TOGETHER_API_KEY_DEVELOPMENT=your_development_api_key_here
# TOGETHER_API_KEY_TESTING=your_testing_api_key_here
# TOGETHER_API_KEY_PRODUCTION=your_production_api_key_here

# API Configuration
TOGETHER_API_URL=https://api.together.xyz/v1
TOGETHER_API_TIMEOUT=120

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/boss
VECTOR_DB_URL=postgresql://user:password@localhost:5432/boss_vectors

# Web Interface Configuration
WEB_HOST=0.0.0.0
WEB_PORT=8000
DEBUG=True

# Boss Configuration
BOSS_DOCS_DIR=/path/to/your/documentation/directory
BOSS_IMPLEMENTATION_DIR=/path/to/output/implementation/directory
LOG_LEVEL=INFO 